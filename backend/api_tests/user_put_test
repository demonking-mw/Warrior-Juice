import sys
import requests

BASE = "http://127.0.0.1:5000"

response = requests.post(
    BASE + "/user",
    json={
        "user_name": "jocelyn",
        "pwd": "100",
    },
    timeout=35,
)
if response.json().get("status"):
    print("complete")
    sys.exit(0)
else:
    print("failed")
    print(response.json())
    sys.exit(3)
    